{
  "name": "@soybeanjs/vite-plugin-vue-page-route",
  "version": "0.0.5",
  "description": "A vite plugin for vue, auto generate route info by page",
  "author": {
    "name": "Soybean",
    "email": "honghuangdc@gmail.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/vite-plugin-vue-page-route",
  "repository": {
    "url": "https://github.com/soybeanjs/vite-plugin-vue-page-route.git"
  },
  "keywords": [
    "soybeanjs",
    "soybean",
    "vite",
    "vite-plugin",
    "vite-plugin-vue",
    "vite-plugin-vue-page-route",
    "page-route",
    "directory-route",
    "file-based-route"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint . --fix",
    "format": "soy prettier-format",
    "commit": "soy git-commit",
    "cleanup": "soy cleanup",
    "update-pkg": "soy update-pkg",
    "publish-pkg": "pnpm -r publish --access public",
    "typecheck": "tsc --noEmit",
    "release": "soy release && pnpm build && pnpm publish-pkg",
    "prepare": "pnpm -r run stub"
  },
  "dependencies": {
    "@swc/core": "1.3.62",
    "chokidar": "3.5.3",
    "execa": "7.1.1",
    "fast-glob": "3.2.12",
    "fs-extra": "11.1.1",
    "kolorist": "1.8.0",
    "magic-string": "0.30.0",
    "micromatch": "4.0.5"
  },
  "devDependencies": {
    "@soybeanjs/cli": "0.5.1",
    "@types/fs-extra": "11.0.1",
    "@types/micromatch": "4.0.2",
    "@types/node": "20.2.5",
    "@types/throttle-debounce": "5.0.0",
    "esbuild-register": "3.4.2",
    "eslint": "8.42.0",
    "eslint-config-soybeanjs": "0.4.8",
    "simple-git-hooks": "2.8.1",
    "tsx": "3.12.7",
    "typescript": "5.1.3",
    "unbuild": "1.2.1",
    "vite": "4.3.9"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soybean git-commit-verify",
    "pre-commit": "pnpm lint-staged"
  }
}
